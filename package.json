{
	"name": "timetracker",
	"productName": "TimeTracker",
	"version": "0.9.24",
	"author": {
		"name": "DerKommentator",
		"email": "layton.prof@proton.me"
	},
	"description": "Electron Project to track your work time",
	"private": true,
	"main": "electron/main.cjs",
	"scripts": {
		"dev:svelte": "vite dev",
		"dev:electron": "cross-env APP_DEV=true concurrently --kill-others \"vite dev\" \"electron ./electron/main.cjs --trace-warnings\"",
		"build:svelte": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"package": "npm run build && electron-forge package",
		"make": "npm run build && electron-forge make",
		"test:electron": "cross-env APP_DEV=true APP_TESTING=true xvfb-maybe -- npx playwright test",
		"test:linux:electron": "cross-env APP_DEV=true APP_TESTING=true xvfb-run --auto-servernum --server-args='-screen 0 1280x960x24' -- npx playwright test",
		"test:sveltecomp": "vitest",
		"coverage": "vitest run --coverage",
		"build:electron:windows": "electron-builder -w --x64 --config build.config.json --publish never",
		"build:electron:linux": "electron-builder -l --x64 --config build.config.json --publish never",
		"build:electron": "electron-builder -wl --x64 --config build.config.json --publish never",
		"deploy:electron": "electron-builder -wl --x64 --config build.config.json --publish always -c.releaseInfo.releaseNotesFile=release-notes.md",
		"build": "cross-env APP_DEV=false APP_TESTING=false npm run build:svelte && npm run build:electron",
		"typesafe-i18n": "typesafe-i18n"
	},
	"devDependencies": {
		"@floating-ui/dom": "^1.6.3",
		"@json2csv/plainjs": "^7.0.6",
		"@juggle/resize-observer": "^3.4.0",
		"@playwright/test": "^1.42.1",
		"@skeletonlabs/skeleton": "^2.9.0",
		"@skeletonlabs/tw-plugin": "^0.1.0",
		"@sveltejs/adapter-auto": "^2.1.1",
		"@sveltejs/adapter-static": "^2.0.3",
		"@sveltejs/kit": "^1.30.4",
		"@tabler/icons-svelte": "2.30.0",
		"@tailwindcss/forms": "^0.5.7",
		"@tailwindcss/typography": "^0.5.10",
		"@testing-library/svelte": "^4.1.0",
		"@testing-library/user-event": "^14.5.2",
		"@types/downloadjs": "^1.4.6",
		"@types/node": "^20.11.28",
		"@typescript-eslint/eslint-plugin": "^5.62.0",
		"@typescript-eslint/parser": "^5.62.0",
		"autoprefixer": "^10.4.18",
		"chart.js": "^4.4.2",
		"chartjs-adapter-moment": "^1.0.1",
		"concurrently": "^8.2.2",
		"cross-env": "^7.0.3",
		"dexie": "^4.0.1-rc.1",
		"dexie-export-import": "^4.1.1",
		"downloadjs": "^1.4.7",
		"electron": "^26.6.10",
		"electron-builder": "^24.13.3",
		"electron-context-menu": "^3.6.1",
		"electron-reload": "^2.0.0-alpha.1",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^8.10.0",
		"eslint-plugin-svelte": "^2.35.1",
		"highlight.js": "^11.9.0",
		"jsdom": "^22.1.0",
		"loglevel": "^1.9.1",
		"moment": "^2.30.1",
		"playwright": "^1.38.1",
		"postcss": "^8.4.35",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"svelte": "^3.59.2",
		"svelte-check": "^3.6.7",
		"tailwindcss": "^3.4.1",
		"tslib": "^2.6.2",
		"typesafe-i18n": "^5.26.2",
		"typescript": "^5.4.2",
		"vite": "^4.5.3",
		"vite-plugin-tailwind-purgecss": "^0.1.4",
		"vitest": "^0.34.6",
		"xvfb-maybe": "^0.2.1"
	},
	"type": "module",
	"dependencies": {
		"electron-serve": "^1.3.0",
		"electron-updater": "^6.1.8",
		"electron-window-state": "^5.0.3"
	}
}
