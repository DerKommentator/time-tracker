{
	"name": "timetracker",
	"productName": "TimeTracker",
	"version": "0.9.9",
	"author": {
		"name": "Layton",
		"email": "test@test.de"
	},
	"description": "Electron Project to track your work time",
	"private": true,
	"main": "electron/main.cjs",
	"scripts": {
		"dev:svelte": "vite dev",
		"dev:electron": "cross-env APP_DEV=true concurrently --kill-others \"vite dev\" \"electron ./electron/main.cjs --trace-warnings\"",
		"build:svelte": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"package": "npm run build && electron-forge package",
		"make": "npm run build && electron-forge make",
		"test": "npx playwright test",
		"dist": "npm run build && electron-builder -wl",
		"build:electron": "electron-builder -wl --x64 --config build.config.json --publish never",
		"deploy:electron": "electron-builder -wl --x64 --config build.config.json --publish always",
		"build": "cross-env APP_DEV=false npm run build:svelte && npm run build:electron"
	},
	"devDependencies": {
		"@floating-ui/dom": "^1.5.1",
		"@playwright/test": "^1.38.0",
		"@skeletonlabs/skeleton": "^2.0.0",
		"@skeletonlabs/tw-plugin": "^0.1.0",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/adapter-static": "^2.0.3",
		"@sveltejs/kit": "^1.5.0",
		"@tabler/icons-svelte": "2.30.0",
		"@tailwindcss/forms": "^0.5.4",
		"@tailwindcss/typography": "^0.5.9",
		"@types/node": "^20.6.0",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"autoprefixer": "^10.4.15",
		"chart.js": "^4.4.0",
		"concurrently": "^8.2.1",
		"cross-env": "^7.0.3",
		"dexie": "^4.0.1-alpha.25",
		"electron": "^26.2.0",
		"electron-builder": "^24.6.4",
		"electron-context-menu": "^3.6.1",
		"electron-reload": "^2.0.0-alpha.1",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte": "^2.26.0",
		"highlight.js": "^11.8.0",
		"playwright": "^1.38.0",
		"postcss": "^8.4.28",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.8.1",
		"svelte": "^3.54.0",
		"svelte-check": "^3.0.1",
		"tailwindcss": "^3.3.3",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^4.3.0",
		"vite-plugin-tailwind-purgecss": "^0.1.3"
	},
	"type": "module",
	"dependencies": {
		"electron-serve": "^1.1.0",
		"electron-updater": "^6.1.4",
		"electron-window-state": "^5.0.3"
	}
}
